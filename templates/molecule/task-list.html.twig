<ul class="task-list list-unstyled">
    {% for task in taskList.getTasks %}
        {% include 'atom/task-list-entry.html.twig' with { task : task, showProject : true } only %}
    {% endfor %}
</ul>
<p class="text-muted">{{ taskList.getIncomplete|length }} tasks open, {{ taskList.getCompleted|length }} done ({{ taskList.getTasks|length }} total).</p>
